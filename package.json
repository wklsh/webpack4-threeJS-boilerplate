		"@fullhuman/postcss-purgecss": "^1.2.0",
		"autoprefixer": "^9.6.1",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^2.1.1",
		"cssnano": "^4.1.10",
		"dotenv": "^8.2.0",
		"eslint": "^6.1.0",
