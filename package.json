{
	"name": "webpack-boilerplate",
	"version": "1.0.0",
	"description": "Static HTML webpack 4 boilerplate",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=development webpack-dev-server --mode development --config webpack/webpack.dev.js",
		"build": "NODE_ENV=production webpack --mode production --config webpack/webpack.prod.js",
		"deploy-production": "NODE_ENV=production webpack --mode production --config webpack/webpack.deploy.js",
		"deploy-staging": "NODE_ENV=staging webpack --mode production --config webpack/webpack.deploy.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wklsh/webpack4-html-static"
	},
	"keywords": [],
	"author": "WKLSH",
	"license": "ISC",
